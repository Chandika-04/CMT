  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Django",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}\\manage.py",
      "args": [
        "runserver"
      ],
      "env": {
        "DJANGO_DEBUG": "TRUE",
        "DJANGO_EMAIL_LIVE": "False"
      },
      "django": true
    },
    {
      "name": "Bridge to Kubernetes",
      "type": "bridge-to-kubernetes.configuration",
      "request": "launch"
    }
  ]
}
